/* PLAYER MOVEMENT */

     function clonekick(player){
        players = room.getPlayerList();
        for (i = 0; i < players.length-1; i++){
            if (player.name == players[i].name){
                room.kickPlayer(player.id,"⛔ 𝐘𝐚 𝐡𝐚𝐲 𝐮𝐧 𝐣𝐮𝐠𝐚𝐝𝐨𝐫 𝐜𝐨𝐧 𝐞𝐬𝐭𝐞 𝐧𝐨𝐦𝐛𝐫𝐞.",false);
            }
        }
    }

