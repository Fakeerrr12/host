	localStorage.getItem(getAuth(player)) ? stats = JSON.parse(localStorage.getItem(getAuth(player))) : stats = [0, 0, 0, 0, "0.00", 0, 0, 0, 0, "0.00"];
        room.sendAnnouncement("[] El jugador " + player.name + " mostr贸 sus stats! [!mostrarstats]",null,0xFF7900,"bold")
	room.sendAnnouncement("[] Stats de " + player.name + ":  Partidos Jugados: " + stats[Ss.GA] + ",  Victorias: " + stats[Ss.WI] + ",  Derrotas: " + stats[Ss.LS] + ", WR: " + stats[Ss.WR] + "%, 斤 Goles: " + stats[Ss.GL] + ",  Asistencias: " + stats[Ss.AS] + ",  GK: " + stats[Ss.GK] + ",  Vallas: " + stats[Ss.CS] + ",  CS%: " + stats[Ss.CP] + "%", null,0x73EC59,"bold");
