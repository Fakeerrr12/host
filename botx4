	room.sendAnnouncement(" !别 金筐 拆 桂 桂娥 别 梆拣吼火别拣.", player.id,0xFFF992,"bold");
	room.sendAnnouncement(" !梆筐拆别娥拣 金筐 拆 桂拣 梆筐拆别娥拣.", player.id,0xFFF992,"bold");
	room.sendAnnouncement(" !筐火答拣 金筐 拆 桂拣 筐火答拣 别娥金拣火娥桂拆.", player.id,0xFFF992,"bold");
	room.sendAnnouncement("---------------------------------------------------", player.id,0x4A3FF5);
				}, 500);
        setTimeout(function(){
        room.sendAnnouncement("---------------------------------------------------", player.id,0x60C6FF,"bold");
        room.sendAnnouncement("漏- ｐ筐拣答筐吼别 金拣 葛拆拆", player.id,0xFFC375,"bold");
        room.sendAnnouncement(" 火娥  '!别娥梆拣筐' 金筐 拆答娥 桂 梆桂娥梆娥拣火拆 别拆 叼拣.", player.id, 0x73EC59,"bold");
        room.sendAnnouncement("---------------------------------------------------", player.id,0x60C6FF,"bold");
				}, 1700);


	if (localStorage.getItem(player.auth) != null) {
		if (JSON.parse(localStorage.getItem(player.auth))[Ss.RL] == "master" || JSON.parse(localStorage.getItem(player.auth))[Ss.RL] == "admin") {
			room.setPlayerAdmin(player.id, true);
			room.sendAnnouncement((JSON.parse(localStorage.getItem(player.auth))[Ss.RL] == "Admin" ? "Master " : "") + player.name + " 隆火筐  金拣火拆 拣筐别拆 拆 桂 桂!",null,0xFF7900,"bold");
        if (localStorage.getItem(getAuth(player))){
            if (JSON.parse(localStorage.getItem(getAuth(player)))[Ss.RL] == "vip"){
			room.sendAnnouncement((JSON.parse(localStorage.getItem(player.auth))[Ss.RL] == "vip" ? "master " : "VIP ") + player.name + " se uni贸 a el server !",null,0xFFF992,"bold");
				}
			}
		}
	}
}

